<?xml version="1.0"?>
<FindBugsFilter>
    <!-- class generated by ANTLR - so can't fix the issues -->
    <Match>
        <Class name="com.butterfill.opb.plsql.translation.PlsqlLexer"/>
    </Match>

    <!-- class generated by ANTLR -->
    <Match>
        <Class name="com.butterfill.opb.plsql.translation.PlsqlParser"/>
    </Match>

    <!-- class generated by ANTLR -->
    <Match>
        <Class name="com.butterfill.opb.plsql.translation.PlsqlTreeParser"/>
    </Match>

    <!--
        overridable method opb_datatype is called from the constructor - but it needs to
        be public so that it can be called by the parser.
        rather than make the mothos final, comments have been added to the class to warn
        that care must be taken when overriding the opb_datatype method.
    -->
    <Match>
        <Class name="com.butterfill.opb.plsql.translation.PlsqlCallParameter"/>
        <Method name="opb_datatype"/>
        <BugCode name="NP"/>
    </Match>

    <!--
        we use the keySet iterator rather than the more performant entrySet.
        performance isn't an issue and it's easier to use the key in logging calls etc
    -->
    <Match>
        <Class name="com.butterfill.opb.plsql.translation.OpbComment"/>
        <Method name="applyElement"/>
        <BugCode name="WMI"/>
    </Match>

    <!--
        the input stream created by this method might not be closed.
        there is so little chance that the input stream won't be closed it's not worth
        complicating the code to be 100% sure
    -->
    <Match>
        <Class name="com.butterfill.opb.plsql.translation.DatatypeMap"/>
        <Method name="initDatatypeMap" params="" returns="void"/>
        <BugCode name="OBL"/>
    </Match>

    <!--
        we have a switch with no default.
        no need for a default. if we don't know what we're reading, ignore it
    -->
    <Match>
        <Class name="com.butterfill.opb.plsql.translation.DatatypeMap"/>
        <Method name="initDatatypeMap" params="java.io.InputStream" returns="void"/>
        <BugCode name="SF"/>
    </Match>

</FindBugsFilter>
